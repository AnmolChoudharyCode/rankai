{
  "name": "jm-rank",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:dev": "cross-env NODE_ENV=development next dev",
    "dev:uat": "dotenv -e .env.uat -- cross-env NEXT_PUBLIC_ENV=uat next dev",
    "dev:prod": "cross-env NODE_ENV=production next dev",
    "build": "cross-env NEXT_TURBOPACK_EXPERIMENTAL_USE_SYSTEM_TLS_CERTS=1 next build",
    "build:dev": "cross-env NODE_ENV=development next build",
    "build:uat": "dotenv -e .env.uat -- cross-env NEXT_PUBLIC_ENV=uat next build",
    "build:prod": "cross-env NODE_ENV=production next build",
    "start": "next start",
    "start:dev": "cross-env NODE_ENV=development next start",
    "start:uat": "cross-env NEXT_PUBLIC_ENV=uat next start",
    "start:prod": "cross-env NODE_ENV=production next start",
    "setup:env": "node scripts/setup-env.js",
    "lint": "eslint"
  },
  "dependencies": {
    "lottie-react": "^2.4.1",
    "next": "16.0.5",
    "react": "19.2.0",
    "react-dom": "19.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9",
    "eslint-config-next": "16.0.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
